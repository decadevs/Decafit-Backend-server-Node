    "target": "es2019",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true, 
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  },
  "include": ["src", "views"]
}
